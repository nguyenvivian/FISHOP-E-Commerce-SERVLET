<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>FISHOP-Servlet-2</display-name>
  
  <servlet>
        <servlet-name>DisplayProducts</servlet-name>
        <servlet-class>DisplayProducts</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>FetchProduct</servlet-name>
        <servlet-class>FetchProduct</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>ProductPage</servlet-name>
        <servlet-class>ProductPage</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>ShoppingCart</servlet-name>
        <servlet-class>ShoppingCart</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>Checkout</servlet-name>
        <servlet-class>Checkout</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>SubmitOrder</servlet-name>
        <servlet-class>SubmitOrder</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>GetTax</servlet-name>
        <servlet-class>GetTax</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>AddShipping</servlet-name>
        <servlet-class>AddShipping</servlet-class>
  </servlet>
  
  <servlet-mapping>
        <servlet-name>DisplayProducts</servlet-name>
        <url-pattern>/index.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
        <servlet-name>FetchProduct</servlet-name>
        <url-pattern>/fetch-product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
        <servlet-name>ProductPage</servlet-name>
        <url-pattern>/product-page</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
        <servlet-name>ShoppingCart</servlet-name>
        <url-pattern>/shopping-cart</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
  		<servlet-name>Checkout</servlet-name>
        <url-pattern>/checkout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  		<servlet-name>SubmitOrder</servlet-name>
        <url-pattern>/submit-order</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  		<servlet-name>GetTax</servlet-name>
        <url-pattern>/get-tax</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  		<servlet-name>AddShipping</servlet-name>
        <url-pattern>/add-shipping</url-pattern>
  </servlet-mapping>
  
    
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>